# Google AI API Configuration
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key_here

# Database
DATABASE_URL=mysql://user:password@localhost:3306/database

# JWT Secret for authentication (generate a random string)
JWT_SECRET=your-secret-key-here-change-in-production

# OAuth Configuration (Manus OAuth Portal)
# For development, you can use oauth.manus.app
# For production, use your own OAuth server
OAUTH_SERVER_URL=https://oauth.manus.app
VITE_APP_ID=your-app-id
VITE_OAUTH_PORTAL_URL=https://oauth.manus.app

# Production settings
NODE_ENV=development
PORT=3000

# Owner OpenID (optional)
OWNER_OPEN_ID=

# Legacy Forge API (optional - not used with Google AI)
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=

